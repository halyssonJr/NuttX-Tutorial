#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_LVGL_ENCONDER
	tristate "LVGL Encondder demo"
	default n
	---help---
		Enable the LVGL Encondder demo

if EXAMPLES_LVGL_ENCONDER

config EXAMPLES_LVGL_ENCONDER_PROGNAME
	string "Program name"
	default "lvencon"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXAMPLES_LVGL_ENCONDER_PRIORITY
	int "Task priority"
	default 100

config EXAMPLES_LVGL_ENCONDER_STACKSIZE
	int "Stack size"
	default DEFAULT_TASK_STACKSIZE

endif
